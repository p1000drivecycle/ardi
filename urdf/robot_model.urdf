<?xml version="1.0"?>
    <robot name="ardi" xmlns:xacro="http://www.ros.org/wiki/xacro" >

    <!-- v1.0.0 - Initial stable version for simulation with Gazebo Harmonic and ROS 2 Jazzy -->
    <xacro:include filename="$(find ardi)/urdf/robot_plugins.gazebo" />

    <xacro:include filename="inertial_macros.xacro" />

    <!-- Define robot constants 
     chassis y axis -->
    <xacro:property name="base_width" value="0.29"/>
    <!-- chassis x axis -->
    <xacro:property name="base_length" value="0.39"/>
    <!-- chassis z axis -->
    <xacro:property name="base_height" value="0.05"/>
    <!-- drive wheel constants -->
    <xacro:property name="wheel_radius" value="0.05"/>
    <xacro:property name="wheel_width" value="0.025"/>
    <!-- gap between chassis and inside wheel TBC -->
    <xacro:property name="wheel_ygap" value="0.020"/>
    <!-- wheel radius/vertical offset TBC -->
    <xacro:property name="wheel_zoff" value="0.00"/>
    <!--distance from centre of baselink along the x axis TBC -->
    <xacro:property name="wheel_xoff" value="0.15"/>

    <!-- castor wheel constants all wheel constants should be positive  -->
    <xacro:property name="wheel_castor_radius" value="0.05"/>
    <xacro:property name="wheel_castor_width" value="0.04"/>
    <!-- gap between chassis and inside wheel TBC -->
    <xacro:property name="wheel_castor_ygap" value="0.025"/>
    <!-- wheel radius vertical offset TBC -->
    <xacro:property name="wheel_castor_zoff" value="0.00"/>
    <!-- distance from centre of baselink along the x axis TBC -->
    <xacro:property name="wheel_castor_xoff" value="0.15"/>

    <!-- base footprint -->
    <link name="base_link"></link>  

    <!-- chassis joint -->
    <joint name="chassis_joint" type="fixed">
        <parent link="base_link"/>
        <child link="chassis"/>
        <origin xyz="0 0 ${(wheel_radius+wheel_zoff)}" rpy="0 0 0"/>
    </joint>

    <!-- chassis link -->
    <link name="chassis">
        <visual>
            <geometry>
                <box size="${base_length} ${base_width} ${base_height}"/>
            </geometry>
            <material name="base_link-material">
                <color rgba="0.011612245176281512 0.5028864580233624 0.018500220124016652 1"/>
            </material>
        </visual>
            <collision>
            <geometry>
                <box size="${base_length} ${base_width} ${base_height}"/>
            </geometry>
            </collision>
        <xacro:box_inertia mass="4" y="${base_width}" x="${base_length}" z="${base_height}"/>
    </link>

    <!-- wheel joints and links -->
    <xacro:macro name="wheel" params="prefix x_reflect y_reflect">
        <joint name="${prefix}_joint" type="continuous">
            <parent link="chassis"/>
            <child link="${prefix}_link"/>
            <origin xyz="${x_reflect*wheel_xoff} ${y_reflect*(base_width/2+wheel_ygap)} ${-wheel_zoff}" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
        </joint>
    
        <link name="${prefix}_link">
            <visual>
                <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
                <geometry>
                    <cylinder radius="${wheel_radius}" length="${wheel_width}"/>
                </geometry>
                <material name="Gray">
                    <color rgba="0.5 0.5 0.5 1.0"/>
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
                <geometry>
                    <cylinder radius="${wheel_radius}" length="${wheel_width}"/>
                </geometry>
            </collision>
            <xacro:cylinder_inertia mass="0.035" radius="${wheel_radius}" length="${wheel_width}"/>
        </link>        
    </xacro:macro>

    <xacro:wheel prefix="wheel_left" x_reflect="1" y_reflect="1"/>
    <xacro:wheel prefix="wheel_right" x_reflect="1" y_reflect="-1"/>    

    <!-- Castor wheel joints and links -->
    <xacro:macro name="wheel_castor" params="prefix x_reflect y_reflect">
        <joint name="${prefix}_joint" type="continuous">
            <parent link="chassis"/>
            <child link="${prefix}_link"/>
            <origin xyz="${x_reflect*wheel_castor_xoff} ${y_reflect*(base_width/2+wheel_castor_ygap)} ${-wheel_castor_zoff}" rpy="0 0 0"/>
            <axis xyz="0 1 0"/>
        </joint>

        <link name="${prefix}_link">
            <visual>
                <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
                <geometry>
                    <cylinder radius="${wheel_castor_radius}" length="${wheel_castor_width}"/>
                </geometry>
                <material name="Gray">
                    <color rgba="0.5 0.5 0.5 1.0"/>
                </material>
            </visual>
            <collision>
                <origin xyz="0 0 0" rpy="${pi/2} 0 0"/>
                <geometry>
                    <cylinder radius="${wheel_castor_radius}" length="${wheel_castor_width}"/>
                </geometry>
            </collision>
            <xacro:cylinder_inertia mass="0.05" radius="${wheel_castor_radius}" length="${wheel_castor_width}"/>
        </link>
    </xacro:macro>

    <xacro:wheel_castor prefix="wheel_castor_left" x_reflect="-1" y_reflect="1"/>
    <xacro:wheel_castor prefix="wheel_castor_right" x_reflect="-1" y_reflect="-1"/>


